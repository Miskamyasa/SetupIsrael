{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  },
  "include": [
    ".eslintrc.cjs",
    "astro.config.mjs",
    "astro-i18next.config.ts",
    "src/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.astro"
  ]
}
